<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body>

    <header>
        <%- include('../partials/header'); %>
    </header>

    <div class="container" style="margin-top: 50px;">
    </div>

    <table class="table table-borderless container">
        <thead>
            <tr>
                <td>
                    <button class="btn btn-primary" type="submit">Business details</button>
                </td>
            </tr>
        </thead>
        <thead>
            <tr>
                <td>
                    <h5>
                        <p>Unique ID:</p> 
                    </h5>
                    <p class="label label-info" id="_uniqueIdBus"><%= uniqueId %></p>
                </td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <div>
                    <td>
                        <label for="_company">Company name :</label>
                        <p class="label label-info" id="_company"><%= busDetails.companyName %></p>
                    </td>
                </div>
            </tr>
            <tr>
                <div>
                    <td> 
                        <label for="role">Role:</label>
                        <p class="label label-info" id="role"><%= busDetails.position %></p>
                    </td>
                </div>
            </tr>
            <tr>
                <div>
                    <td>
                        <label for="yrs">Years:</label>
                        <p class="label label-info" id="yrs"><%= busDetails.year %></p>
                    </td>
                </div>
            </tr>
            <tr>
                <div>
                    <td>
                        <label for="slry">salary:</label>
                        <p class="label label-info" id="slry"><%= busDetails.salary %></p>
                    </td>
                </div>
            </tr>

        </tbody>
    </table>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>

</html>